    "prepublish": "npm run clean && npm run compile"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "jest": "23.4.2",
