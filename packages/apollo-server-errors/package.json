    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testRegex": "src/__tests__/.*$",
    "globals": {
      "ts-jest": {
        "skipBabel": true
