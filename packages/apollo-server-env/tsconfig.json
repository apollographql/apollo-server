{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "removeComments": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/__tests__/*", "**/__mocks__/*"],
  "types": []
}
